# The following is the default entry point. This section will determine 
# what system we are on and call make again with the correct flags set.

@WITH_IDL_TRUE@all : 
@WITH_IDL_TRUE@	@echo "OS type detected: "`uname`
@WITH_IDL_TRUE@	@case `uname` in \
@WITH_IDL_TRUE@	   "Darwin") make gpulib ;; \
@WITH_IDL_TRUE@	   "Linux" ) make gpulib ;; \
@WITH_IDL_TRUE@	   *) echo "This system is not supported" ;; \
@WITH_IDL_TRUE@       esac

@WITH_IDL_TRUE@gpulib:
@WITH_IDL_TRUE@	gcc $(C_FLAGS)  -Bsymbolic -o gpulib.so -g gpulib.c -I../vectorOp -I../physicsOp $(srcdir)/../vectorOp/gpuVectorOp.o $(srcdir)/../physicsOp/gpuPhysicsOp.o -L$(RSIIDL_LIBBIN) -lidl $(CUDA_LIBS) $(EMULATION_LIBS) $(GL_LIBS)
@WITH_IDL_TRUE@	sh generate_op.sh
@WITH_IDL_TRUE@	@date

@WITH_IDL_TRUE@html-local:
@WITH_IDL_TRUE@	echo In html target, IDL_PATH is $(IDL_PATH)
@WITH_IDL_TRUE@	idl gpu_make_docs

@WITH_IDL_TRUE@clean: 
@WITH_IDL_TRUE@	rm -f gpulib.o gpulib.so gpulib.sl gpulib.a 

@WITH_IDL_TRUE@check:
@WITH_IDL_TRUE@	idl gpu_run_test2
@WITH_IDL_TRUE@	idl gpu_run_unittests

@WITH_IDL_TRUE@benchmark:
@WITH_IDL_TRUE@	 idl -e gpu_benchmark

@WITH_IDL_TRUE@install-exec-local: gpulib
@WITH_IDL_TRUE@	mkdir -p $(prefix)/IDL/lib
@WITH_IDL_TRUE@	cp gpulib.so gpulib.dlm lib/* $(prefix)/IDL/lib
@WITH_IDL_TRUE@	mkdir -p $(prefix)/IDL/docs
@WITH_IDL_TRUE@	cp -r docs/* $(prefix)/IDL/docs
@WITH_IDL_TRUE@	mkdir -p $(prefix)/IDL/demos
@WITH_IDL_TRUE@	cp -r demos/* $(prefix)/IDL/demos
